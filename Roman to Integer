class Solution:
    def romanToInt(self, s: str) -> int:

        

        Map={
            "I":1,
            "V":5,
            "X":10,
            "L":50,
            "C":100,
            "D":500,
            "M":1000
        }
        num=0
        for i in range(len(s)-1,-1,-1):
            if i<len(s)-1 and Map[s[i]]<Map[s[i+1]]:
                num-=Map[s[i]]
                continue
            
            num+=Map[s[i]]

        return num


        

        
    

        
